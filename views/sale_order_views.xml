<odoo>
    <record id="view_order_form_inherit_project" model="ir.ui.view">
        <field name="name">sale.order.form.inherit.project</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet//group[@name='sale_order_dates']" position="after">
                <group string="Proyecto">
                    <field name="project_id" domain="[('obra_nr', '!=', False)]"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>

