<odoo>
    <record id="view_stock_picking_form_inherit_project" model="ir.ui.view">
        <field name="name">stock.picking.form.inherit.project</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet//group[@name='picking_information']" position="after">
                <group string="Proyecto">
                    <field name="project_id" domain="[('obra_nr', '!=', False)]"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
